<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
        <solid android:color="#FFE1D9BE" />
        <!-- 点击后描边 -->
        <stroke android:width="1dp" android:color="#FFE1D9BE" />
        <!-- 点击后圆角 -->
        <corners android:radius="100dip" />
    </shape></item>



    <item android:state_focused="true"><shape>
        <stroke android:width="1dp" android:color="#FFCED198" />
        <!--半径-->
        <corners android:radius="5dp" />
    </shape></item>
    <item><shape>
        <!--<点击前颜色>-->
        <solid android:color="#FFCED198" />
        <!--点击前描边-->
        <stroke android:width="0dp" android:color="#FFCED198" />
        <!--点击前圆角-->
        <corners android:radius="0dip" />
    </shape></item>




</selector>